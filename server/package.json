{
  "name": "server",
  "version": "1.0.0",
  "description": "backend server for ecommerce web app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon",
    "frontend": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --watch",
    "build": "docker build -t shoopp-dev:1.0.0 -f Dockerfile ."
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/routes/*.test.js"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeek00/Ecommerce-STORE"
  },
  "keywords": [
    "node",
    "ecommerce",
    "react",
    "express"
  ],
  "author": "Isaac Isewede",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.2",
    "node-cron": "^3.0.3",
    "verifalia": "^4.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  }
}
